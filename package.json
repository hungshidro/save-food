{
  "name": "reactnativeBase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "debug": "react-native start --experimental-debugger",
    "pi": "cd ios && pod install && cd ..",
    "add-screen": "node scripts/add-screen.js",
    "clean": "watchman watch-del-all && killall -9 node && rm -rf yarn.lock package-lock.json node_modules ios/Pods ios/Podfile.lock android/app/build && yarn && cd ios && pod update && cd .. && yarn start -- --reset-cache",
    "apk": "cd android && ./gradlew assembleRelease && cd .. && open android/app/build/outputs/apk/release",
    "android:clean": "cd android && ./gradlew clean && ./gradlew cleanBuildCache && cd ..",
    "ios:clean": "cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && pod cache clean --all && pod deintegrate && pod setup && pod install && cd .."
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.6.12",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@tanstack/react-query": "^5.59.0",
    "axios": "^1.7.7",
    "i18next": "^23.15.1",
    "install": "^0.13.0",
    "moment": "^2.30.1",
    "npm": "^10.8.3",
    "react": "18.3.1",
    "react-i18next": "^15.0.2",
    "react-native": "0.75.3",
    "react-native-blob-util": "^0.19.11",
    "react-native-confirmation-code-field": "^7.4.0",
    "react-native-keyboard-controller": "^1.13.4",
    "react-native-localize": "^3.2.1",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.15.2",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.7.1",
    "react-native-toast-message": "^2.2.1",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
